{
	"name": "@data-prism/core",
	"version": "0.1.2",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "npm run clean && rollup -c && cp index.d.ts dist/",
		"clean": "rm -rf dist",
		"lint": "eslint src/ test/",
		"prepublishOnly": "npm run lint && npm run test && npm run build",
		"test": "NODE_ENV=test vitest run --coverage",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "Jake Sower",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"@vitest/coverage-istanbul": "^3.0.4",
		"@vitest/coverage-v8": "^3.2.4",
		"rollup": "^4.28.1",
		"vitest": "^3.0.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"private": false,
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"dependencies": {
		"@data-prism/expressions": "*",
		"@data-prism/utils": "*",
		"ajv": "^8.17.1",
		"ajv-errors": "^3.0.0",
		"ajv-formats": "^3.0.1",
		"lodash-es": "^4.17.21",
		"uuid": "^10.0.0"
	}
}
