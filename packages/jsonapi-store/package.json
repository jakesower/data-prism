{
	"name": "@data-prism/jsonapi-store",
	"version": "0.0.4",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "npm run clean && rollup -c && cp index.d.ts dist/",
		"clean": "rm -rf dist",
		"format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"start": "npx tsx test/test-server.js",
		"lint": "eslint src/ test/",
		"lint:fix": "eslint --fix src/ test/",
		"test": "start-test 3000 'NODE_ENV=test vitest run --coverage'",
		"test:unit": "NODE_ENV=test vitest run test/unit/ --coverage",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "Jake Sower",
	"devDependencies": {
		"@data-prism/core": "0.1.6",
		"@data-prism/interface-tests": "*",
		"@data-prism/jsonapi-server": "^0.0.11",
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"axios": "^1.11.0",
		"express": "^5.1.0",
		"rollup": "^4.48.0",
		"start-server-and-test": "^2.0.13",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"private": true,
	"files": [
		"dist/",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"dependencies": {
		"@data-prism/expressions": "^0.0.15",
		"@data-prism/utils": "^0.0.7",
		"lodash-es": "^4.17.21",
		"vite": "7.1.3"
	}
}
