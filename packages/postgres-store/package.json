{
	"name": "@data-prism/postgres-store",
	"version": "0.0.24",
	"type": "module",
	"main": "./dist/postgres-store.cjs.js",
	"module": "./dist/postgres-store.esm.js",
	"types": "./dist/postgres-store.d.ts",
	"exports": {
		".": {
			"types": "./dist/postgres-store.d.ts",
			"import": "./dist/postgres-store.esm.js",
			"require": "./dist/postgres-store.cjs.js"
		}
	},
	"scripts": {
		"build": "npm run clean && rollup -c && cp postgres-store.d.ts dist/",
		"clean": "rm -rf dist",
		"format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
		"lint": "eslint src/ test/",
		"lint:fix": "eslint --fix src/ test/",
		"prepublishOnly": "npm run lint && npm run test",
		"test": "NODE_ENV=test vitest run --coverage",
		"test:watch": "vitest"
	},
	"keywords": [],
	"author": "Jake Sower",
	"devDependencies": {
		"@data-prism/interface-tests": "*",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"rollup": "^4.28.1",
		"testcontainers": "^10.15.0",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@data-prism/core": "0.1.6",
		"@data-prism/sql-helpers": "*",
		"@data-prism/utils": "^0.0.7",
		"ajv": "^8.17.1",
		"es-toolkit": "^1.26.0",
		"pg": "^8.12.0",
		"vite": "7.0.6"
	}
}
